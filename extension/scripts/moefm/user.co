($)<- define [\jquery]
new
    user = @
    @is_login = ->
        flag = null
        $.ajax do
            url:'http://moe.fm/listen/playlist'
            async: false
        .success (rst)->
            flag := $.isArray rst
        flag
    @do_login = ->
        window.open (chrome.extension.getURL 'login.html'), \com.mcfog.chrome.mmf.login, 'noresize=1,width=240,height=65,top=0,left=0'
        void
    @req_login = ->
        if ! user.is_login!
            user.do_login!
#EOF
